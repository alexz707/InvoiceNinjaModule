{
  "name":              "alexz707/invoiceninja-module",
  "description":       "Zend Framework Module that provides invoiceninja api functionality",
  "type":              "library",
  "license":           "MIT",
  "keywords":          [
    "invoiceninja",
    "module",
    "zf"
  ],
  "homepage":          "https://github.com/alexz707/invoiceninja-module",
  "authors":           [
    {
      "name":     "Alexander Zamponi",
      "email":    "alex_com@gmx.at",
      "homepage": "https://github.com/alexz707/"
    }
  ],
  "require":           {
    "php":                               "^5.6 || ^7.0",
    "symfony/console":                   "^2.3 || ^3.0",
    "zendframework/zend-stdlib":         "^2.7.7 || ^3.0.1",
    "zendframework/zend-hydrator":       "^1.1 || ^2.2.1",
    "zendframework/zend-mvc":            "^2.7.10 || ^3.0.1",
    "zendframework/zend-filter":         "^2.7.1",
    "zendframework/zend-servicemanager": "^2.7.6 || ^3.1"
  },
  "require-dev":       {
    "phpunit/phpunit":                    "^5.7.0",
    "squizlabs/php_codesniffer":          "^2.6.2"
  },
  "autoload":          {
    "psr-0": {
      "InvoiceNinjaModule\\": "src/"
    }
  },
  "autoload-dev":      {
    "psr-0": {
      "InvoiceNinjaModuleTest\\": "tests/InvoiceNinjaModuleTest"
    }
  }
}
